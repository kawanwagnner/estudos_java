package Desafios;

public class EstruturaRepeticao3 {

	public static void main(String[] args) {

		for (int i = 0; i <= 10; i++) {
			for (int j = 0; j < i; j++) {
				System.out.print(j + 1);
			}
			for (int j2 = 0; j2 < i; j2--) {
				System.out.print(j2 + 1);
			}
			System.out.println();
		}
	}

}
